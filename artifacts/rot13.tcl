#!/usr/bin/tclsh

set input ""

set input [binary decode base64 $input]

set input [split $input ""]
set s 13
set output ""

foreach char $input {
    set a [scan $char %c]
    if {$a > 33 && $a < 127} {
        set a [expr {($a + 47 - 33) % 94 + 33}]
    }

    set char [format %c $a]

    append output $char
}

eval $output